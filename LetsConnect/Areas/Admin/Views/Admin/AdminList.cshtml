
@{
    ViewBag.Title = "AdminList";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<script src="~/Scripts/Controller/Menu/MenuController.js"></script>
<script src="~/Scripts/Service/LcService.js"></script>
<script src="~/Scripts/Validations/Common/CommonValidation.js"></script>
<script src="~/Scripts/Controller/Administrator/AdministratorController.js"></script>

<div class="right_col" role="main" ng-controller="AdministratorController" ng-model="Administrator" ng-cloak>
    <div id="content-page">
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">

                <div class="alert alert-success alert-dismissible fade in" role="alert" ng-show="process.isSuccessMsg">
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                    {{process.SuccessMsg}}
                </div>

                <div class="alert alert-danger alert-dismissible fade in" role="alert" ng-show="process.isErrorMsg">
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                    {{process.ErrorMsg}}
                </div>


            </div>
        </div>
        <div @*ng-show="view.isList = false"*@ id="NonList">
            <div class="page-title">
                <div class="title_left">
                    <h3>Super Admin <small>Register</small></h3>
                </div>
                <div class="title_right">
                    <div class="col-md-2 col-sm-2 col-xs-2 form-group pull-right top_search">
                        <div class="input-group">
                            <div class="btn btn-primary" ng-click="BackToList()" value="">Back</div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>
            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <h2>Register Admin</h2>
                            <ul class="nav navbar-right panel_toolbox">
                                <li>
                                    <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                                </li>
                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                                    <ul class="dropdown-menu" role="menu">
                                        <li>
                                            <a href="#">Settings 1</a>
                                        </li>
                                        <li>
                                            <a href="#">Settings 2</a>
                                        </li>
                                    </ul>
                                </li>
                                <li>
                                    <a class="close-link"><i class="fa fa-close"></i></a>
                                </li>
                            </ul>
                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content">
                            <br />
                            <form id="demo-form2" data-parsley-validate class="form-horizontal form-label-left">
                                <div class="form-group">
                                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                                        Full Name <span class="required">*</span>
                                    </label>
                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                        <input limit-to="100" type="text" ng-model="Administrator.fullName" id="full-name" required="required" class="form-control col-md-7 col-xs-12">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">
                                        User Name <span class="required">*</span>
                                    </label>
                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                        <input limit-to="50" type="text" id="last-name" ng-model="Administrator.userName" name="user-name" required="required" class="form-control col-md-7 col-xs-12">
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">
                                        Password <span class="required">*</span>
                                    </label>
                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                        <input type="password" id="last-name" ng-model="Administrator.password" name="user-name" required="required" class="form-control col-md-7 col-xs-12">
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">
                                        Email Id <span class="required">*</span>
                                    </label>
                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                        <input limit-to="100" type="text" id="last-name" ng-model="Administrator.emailID" name="user-name" required="required" class="form-control col-md-7 col-xs-12">
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">
                                        Mobile No <span class="required">*</span>
                                    </label>
                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                        <input limit-to="15" type="text" id="last-name" ng-change="OnlyNumber()" ng-model="Administrator.mobileNo" name="user-name" required="required" class="form-control col-md-7 col-xs-12">
                                    </div>
                                </div>

                                <div class="ln_solid"></div>
                                <div class="form-group">
                                    <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3" ng-hide="process.isloading">

                                        <div type="submit" ng-click="Register(Administrator)" class="btn btn-primary btn-block btn-flat">Submit</div>
                                    </div>
                                    <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3" ng-show="process.isloading">
                                        <img src="~/Content/Template/Images/Loading_32.gif" />
                                    </div>

                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>

        </div>

        <div id="List">
            <div class="page-title">
                <div class="title_left">
                    <h3>SuperAdmin  <small> - Admin</small></h3>
                </div>
                <div class="title_right">
                    <div class="col-md-2 col-sm-2 col-xs-2 form-group pull-right top_search">
                        <div class="input-group">
                            <div  class="btn btn-primary" ng-click="AddNewSection()" value="">Add New</div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="clearfix"></div>
            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <h2>AdminList</h2>
                            <ul class="nav navbar-right panel_toolbox">
                                <li>
                                    <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                                </li>
                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                                    <ul class="dropdown-menu" role="menu">
                                        <li>
                                            <a href="#">Settings 1</a>
                                        </li>
                                        <li>
                                            <a href="#">Settings 2</a>
                                        </li>
                                    </ul>
                                </li>
                                <li>
                                    <a class="close-link"><i class="fa fa-close"></i></a>
                                </li>
                            </ul>
                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content" ng-show="AdministratorList.length > 0">

                            <div id="datatable-responsive_wrapper" class="dataTables_wrapper form-inline dt-bootstrap no-footer">
                                @*<div class="row"><div class="col-sm-6"><div class="dataTables_length" id="datatable-responsive_length"><label>Show <select name="datatable-responsive_length" aria-controls="datatable-responsive" class="form-control input-sm"><option value="10">10</option><option value="25">25</option><option value="50">50</option><option value="100">100</option></select> entries</label></div></div><div class="col-sm-6"><div id="datatable-responsive_filter" class="dataTables_filter"><label>Search:<input type="search" class="form-control input-sm" placeholder="" aria-controls="datatable-responsive"></label></div></div></div><div class="row">*@
                                    <div class="col-sm-12">
                                        <table id="datatable-responsive" class="table table-striped table-bordered dt-responsive nowrap dataTable no-footer dtr-inline" cellspacing="0" width="100%" role="grid" aria-describedby="datatable-responsive_info" style="width: 100%;">
                                            <thead>
                                                <tr role="row">
                                                    <th class="sorting_asc" tabindex="0" aria-controls="datatable-responsive" rowspan="1" colspan="1" style="width: 69px;" aria-sort="ascending" aria-label="First name: activate to sort column descending">Full Name</th>
                                                    <th class="sorting" tabindex="0" aria-controls="datatable-responsive" rowspan="1" colspan="1" style="width: 67px;" aria-label="Last name: activate to sort column ascending">User Name</th>
                                                    <th class="sorting" tabindex="0" aria-controls="datatable-responsive" rowspan="1" colspan="1" style="width: 152px;" aria-label="Position: activate to sort column ascending">Email Id</th>
                                                    <th class="sorting" tabindex="0" aria-controls="datatable-responsive" rowspan="1" colspan="1" style="width: 26px;" aria-label="Age: activate to sort column ascending">Mobile No</th>
                                                    <th class="sorting" tabindex="0" aria-controls="datatable-responsive" rowspan="1" colspan="1" style="width: 62px;" aria-label="Start date: activate to sort column ascending">Is Active</th>
                                                    <th class="sorting" tabindex="0" aria-controls="datatable-responsive" rowspan="1" colspan="1" style="width: 44px;" aria-label="Salary: activate to sort column ascending">Admin Type</th>
                                                    <th class="sorting" tabindex="0" aria-controls="datatable-responsive" rowspan="1" colspan="1" style="width: 146px;" aria-label="E-mail: activate to sort column ascending">Actions</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr role="row" class="odd" ng-repeat="adm in AdministratorList">
                                                    <td>{{adm.fullName}}</td>
                                                    <td>{{adm.userName}}</td>
                                                    <td>{{adm.emailID}}</td>
                                                    <td>{{adm.mobileNo}}</td>
                                                    <td>{{adm.isActive}}</td>
                                                    <td>{{adm.administratorType}}</td>
                                                    <td>
                                                        <div class="btn-group">
                                                            <button type="button" ng-click="Edit(adm)" class="btn btn-primary" aria-label="edit"><i class="fa fa-edit"></i></button>
                                                            <button type="button" ng-click="Delete(adm)" class="btn btn-danger" aria-delete="edit"><i class="fa fa-remove"></i></button>
                                                        </div>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                @*<div class="row"><div class="col-sm-5"><div class="dataTables_info" id="datatable-responsive_info" role="status" aria-live="polite">Showing 1 to 10 of 57 entries</div></div><div class="col-sm-7"><div class="dataTables_paginate paging_simple_numbers" id="datatable-responsive_paginate"><ul class="pagination"><li class="paginate_button previous disabled" id="datatable-responsive_previous"><a href="#" aria-controls="datatable-responsive" data-dt-idx="0" tabindex="0">Previous</a></li><li class="paginate_button active"><a href="#" aria-controls="datatable-responsive" data-dt-idx="1" tabindex="0">1</a></li><li class="paginate_button "><a href="#" aria-controls="datatable-responsive" data-dt-idx="2" tabindex="0">2</a></li><li class="paginate_button "><a href="#" aria-controls="datatable-responsive" data-dt-idx="3" tabindex="0">3</a></li><li class="paginate_button "><a href="#" aria-controls="datatable-responsive" data-dt-idx="4" tabindex="0">4</a></li><li class="paginate_button "><a href="#" aria-controls="datatable-responsive" data-dt-idx="5" tabindex="0">5</a></li><li class="paginate_button "><a href="#" aria-controls="datatable-responsive" data-dt-idx="6" tabindex="0">6</a></li><li class="paginate_button next" id="datatable-responsive_next"><a href="#" aria-controls="datatable-responsive" data-dt-idx="7" tabindex="0">Next</a></li></ul></div></div></div>*@
                            </div>

                        </div>
                        <div class="x_content" ng-hide="AdministratorList.length > 0">
                            <h5 class="text-center">No Records To Dispaly.!</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    <div id="loading">
        <div class="col-md-12 col-sm-12 col-xs-12 text-center">
            <img src="~/Content/Template/Images/loading_128.gif" class="margin-top-17p" />
        </div>
    </div>
    </div>
   

